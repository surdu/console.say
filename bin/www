#!/usr/bin/env node
var debug = require('debug')('lists');
var app = require('../app');
var cs = require('../lib/console.say');

app.set('port', process.env.PORT || 3000);

var server = app.listen(app.get('port'), function() {
  debug('Express server listening on port ' + server.address().port);
});

cs.listen(server);
